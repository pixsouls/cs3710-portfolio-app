<div id="<%= dom_id student %>">
  <p>
    <strong>Name:</strong>
    <%= student.name %>
  </p>

  <p>
    <strong>School email:</strong>
    <%= student.school_email %>
  </p>

  <p>
    <strong>Major:</strong>
    <%= student.major %>
  </p>

  <p>
    <strong>Minor:</strong>
    <%= student.minor %>
  </p>

  <p>
    <strong>Graduation date:</strong>
    <%= student.graduation_date %>
  </p>

  <p>
    <strong>Profile Picture:</strong>
    <% if student.profile_picture.attached? %>
      <%= image_tag student.profile_picture, size: "150x150" %>
    <% else %>
      <img src="https://img.icons8.com/?size=100&id=6644&format=png&color=000000"></img>
    <% end %>
  </p>

</div>